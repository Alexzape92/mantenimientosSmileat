<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.smileat.mantenimientos.VerProgramaController">
    <top>
        <StackPane>
            <HBox alignment="CENTER">
                <Label fx:id="lblTitulo" />
            </HBox>
            <HBox alignment="CENTER_LEFT">
                <ImageView fx:id="imgAtras">
                    <image>
                        <Image url="@img/atras.png" />
                    </image>
                </ImageView>
            </HBox>
        </StackPane>
    </top>
    <center>
        <FlowPane fx:id="calendar" alignment="CENTER" orientation="HORIZONTAL" />
    </center>
</BorderPane>