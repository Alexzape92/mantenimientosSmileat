<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>

<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.smileat.mantenimientos.VerUsuariosController">
    <top>
        <StackPane>
            <HBox alignment="CENTER">
                <Label fx:id="lblTitulo" />
            </HBox>
            <HBox alignment="CENTER_LEFT">
                <ImageView fx:id="imgAtras">
                    <image>
                        <Image url="@img/atras.png" />
                    </image>
                </ImageView>
            </HBox>
        </StackPane>
    </top>
    <center>
        <VBox alignment="TOP_CENTER" spacing="20.0">
            <Label text=""/>
            <HBox alignment="TOP_RIGHT" spacing="20.0">
                <ChoiceBox fx:id="cbUsuarios" />
                <Label text=""/>
            </HBox>
            <TableView fx:id="tbUsuarios" >
                <columns>
                    <TableColumn fx:id="tcNombre" text="NOMBRE"/>
                    <TableColumn fx:id="tcIconos" text="ACCIONES"/>
                </columns>
            </TableView>
        </VBox>
    </center>
</BorderPane>